<?xml version="1.0" encoding="UTF-8"?>

<!-- Utilización de dos espacios de nombres 
     para dos etiquetas producto que tienen 
     estructuras diferentes -->

<!-- Se usan URLs para diferenciar los prefijos -->
<tienda xmlns:t1="url1"
        xmlns:t2="url2">
    
    <t1:producto>
        Un producto
    </t1:producto>

    <!-- Esta etiqueta producto tiene otra estructura-->
    <t2:producto>
        <nombre>Otro producto</nombre>
        <origen>España</origen>
    </t2:producto>

</tienda>
